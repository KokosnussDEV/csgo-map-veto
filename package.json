{
  "name": "csgo-map-veto",
  "version": "2.0.0",
  "private": true,
  "description": "A WEB and desktop app to do CSGO map veto.",
  "license": "GPL-2.0",
  "author": "AkiVer",
  "homepage": "https://github.com/akiver/csgo-map-veto",
  "repository": {
    "type": "git",
    "url": "https://github.com/akiver/csgo-map-veto.git"
  },
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "db": "cd backend && gin --port 3080 run main.go && popd",
    "build": "yarn compile && electron-builder",
    "build:all": "electron-builder -mwl",
    "build:db": "cd backend && go build -ldflags=\"-s -w\"",
    "build:dir": "yarn dist --dir",
    "build:web": "webpack --mode production --config webpack.web.js --progress --profile --colors",
    "compile": "electron-webpack",
    "dev": "webpack-dev-server --hot --config webpack.web.js --open",
    "dev:electron": "electron-webpack dev",
    "lint": "eslint '{src,test,cypress}/**/*.{ts,tsx}'",
    "prettier": "prettier --write \"{src,test,cypress}/**/*.{ts,tsx}\"",
    "t": "yarn test",
    "tw": "yarn test --watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:e2e": "node cypress/run.js",
    "test:e2e:dev": "node cypress/open.js",
    "tsc": "tsc --noEmit",
    "tsc:e2e": "tsc -p cypress/tsconfig.json --noEmit"
  },
  "electronWebpack": {
    "renderer": {
      "webpackConfig": "webpack.renderer.js"
    },
    "whiteListedModules": [
      "react-redux",
      "react-router-dom"
    ]
  },
  "cypress-cucumber-preprocessor": {
    "step_definitions": "cypress/integration"
  },
  "dependencies": {
    "electron-updater": "^4.1.2",
    "source-map-support": "^0.5.13"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@cypress/webpack-preprocessor": "^4.1.0",
    "@svgr/webpack": "^4.3.2",
    "@testing-library/cypress": "^4.0.5",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^8.0.7",
    "@types/jest": "^24.0.16",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "@types/react-redux": "^7.1.1",
    "@types/react-router-dom": "^4.3.4",
    "@types/react-select": "^3.0.0",
    "@types/styled-components": "4.1.18",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.2.0",
    "cypress": "^3.4.1",
    "cypress-cucumber-preprocessor": "1.12.0",
    "devtron": "^1.4.0",
    "dotenv": "^8.0.0",
    "electron": "6.0.0",
    "electron-builder": "21.2.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-is": "^3.0.0",
    "electron-webpack": "^2.7.4",
    "electron-webpack-ts": "^3.2.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.6.1",
    "husky": "^3.0.2",
    "jest": "^24.8.0",
    "jest-styled-components": "^6.3.3",
    "lint-staged": "^9.2.1",
    "prettier": "1.18.2",
    "react": "16.9.0-alpha.0",
    "react-dom": "16.9.0-alpha.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-select": "^3.0.4",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "start-server-and-test": "^1.9.1",
    "styled-components": "^4.3.2",
    "typescript": "^3.5.3",
    "webpack": "^4.39.1",
    "whatwg-fetch": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn tsc && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn prettier",
      "git add"
    ]
  },
  "build": {
    "appId": "com.akiver.csgomapveto",
    "productName": "CSGO Map Veto",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "directories": {
      "buildResources": "public",
      "output": "dist/electron"
    },
    "extraResources": [
      "static/fonts",
      "static/maps"
    ]
  },
  "keywords": [
    "csgo",
    "electron",
    "react",
    "typescript"
  ],
  "devEngines": {
    "node": ">=10.0.0",
    "yarn": "1.16.0"
  }
}
